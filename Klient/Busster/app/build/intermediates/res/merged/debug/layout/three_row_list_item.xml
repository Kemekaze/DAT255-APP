    <TextView
        android:id="@+id/TextViewId"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="?android:attr/listPreferredItemHeight"
        android:textColor="#41404b"
        android:text="id"
        android:textSize="25dp"></TextView>
